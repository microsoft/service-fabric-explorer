{
    "update": {
        "baseUrl": "https://aka.ms/sfx-update",
        "defaultChannel": "stable"
    },
    "logging": {
        "loggers": [
            {
                "name": "app-insights",
                "component": "logging.logger.app-insights",
                "instrumentationKey": "84c71119-d044-4bc2-86b3-d65a8968c241"
            },
            {
                "name": "console",
                "component": "logging.logger.console",
                "logAllProperties": false,
                "logCallerInfo": true
            }
        ]
    },
    "defaultMenu": {
        "darwin": [
            {
                "submenu": [
                    {
                        "role": "quit"
                    }
                ]
            },
            {
                "label": "Edit",
                "submenu": [
                    {
                        "role": "undo"
                    },
                    {
                        "role": "redo"
                    },
                    {
                        "type": "separator"
                    },
                    {
                        "role": "cut"
                    },
                    {
                        "role": "copy"
                    },
                    {
                        "role": "paste"
                    },
                    {
                        "role": "selectall"
                    }
                ]
            },
            {
                "role": "window",
                "submenu": [
                    {
                        "role": "minimize"
                    },
                    {
                        "role": "togglefullscreen"
                    }
                ]
            }
        ]
    },
    "package-manager": {
        "packagesDir": "packages",
        "repos": {
            "public": {
                "name": "public",
                "url": "https://www.myget.org/F/sfx/npm/"
            }
        }
    }
}