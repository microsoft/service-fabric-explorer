{
  "name": "sfx-standalone",
  "productName": "sfx-standalone",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start -- devtools",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:windows": "electron-forge make --platform=win32",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "debug": "electron-forge start --inspect-electron"
  },
  "keywords": [],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Service-Fabric-Explorer-Standalone"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/mainWindow/index.html",
                  "js": "./src/MainWindow/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/yaireo__relative-time": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.7.0",
    "electron": "^17.1.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@azure/msal-node": "^1.7.0",
    "@yaireo/relative-time": "^1.0.1",
    "axios": "^0.26.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.6",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0"
  }
}
